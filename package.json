    "@babel/preset-env": "^7.1.0",
    "@svgr/webpack": "2.4.1",
    "antd": "^3.10.1",
    "axios": "^0.18.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
