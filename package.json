    "babel-plugin-named-asset-import": "^0.2.2",
    "babel-plugin-react-intl": "^3.0.1",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-react-app": "^5.0.4",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "emotion": "^9.2.12",
    "eslint": "5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-react-app": "^3.0.4",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "faker": "^4.1.0",
    "file-loader": "2.0.0",
    "fs-extra": "7.0.0",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "husky": "^1.1.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-pnp-resolver": "1.0.1",
    "jest-resolve": "23.6.0",
    "json-server": "^0.14.0",
    "jsonwebtoken": "^8.3.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "lint-staged": "^7.3.0",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "0.4.3",
    "mobx": "^5.5.0",
    "mobx-devtools-mst": "^0.9.18",
    "mobx-react": "^5.3.1",
    "mobx-react-router": "^3.1.2",
    "mobx-state-tree": "^3.5.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.1.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.0.6",
    "postcss-safe-parser": "4.0.1",
    "prettier": "^1.14.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-app-polyfill": "^0.1.3",
    "react-dev-utils": "^6.0.5",
    "react-dom": "^16.5.2",
    "react-emotion": "^9.2.12",
    "react-hot-loader": "^4.3.11",
    "react-intl": "^2.7.1",
    "react-loadable": "^5.5.0",
    "react-router": "^3.2.1",
    "regenerator-runtime": "^0.12.1",
    "resolve": "1.8.1",
    "sass-loader": "7.1.0",
    "source-map-explorer": "^1.6.0",
    "style-loader": "0.23.0",
    "terser-webpack-plugin": "1.1.0",
    "universal-cookie": "^3.0.4",
    "url-loader": "1.1.1",
    "webpack": "4.19.1",
    "webpack-dev-server": "3.1.9",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.2"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "precommit": "echo Running Prettier and ESLint for staged files...; lint-staged",
    "start": "node scripts/start.js",
    "start-server": "node json-server",
    "build": "echo Building i18n; npm run build-i18n; node scripts/build.js",
    "build-i18n": "rm -rf src/i18n/build/messages; NODE_ENV=production npx babel src > /dev/null; NODE_ENV=production npx babel-node src/i18n/aggregator.js --presets @babel/env",
    "test": "node scripts/test.js",
    "linter": "eslint \"src/**/*.js\" ",
    "prettier": "prettier \"src/**/*.js\" --write"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,css,scss}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  }
}
