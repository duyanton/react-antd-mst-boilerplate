{
  "presets": ["env", "react", "stage-0"],
  "plugins": [
    ["emotion"],
    "lodash",
    "transform-decorators-legacy",
    "transform-async-generator-functions",
    [
      "react-intl",
      {
        "messagesDir": "./src/i18n/build/messages"
      }
    ],
    [
      "import",
      {
        "libraryName": "antd"
      }
    ]
  ]
}
